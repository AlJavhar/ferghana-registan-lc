@using RegistanFerghanaLC.Service.ViewModels.SalaryViewModels;
@model PagedList<SalaryViewModel>
@{
    ViewBag.Tab = 5;
}


<div class="card card-flush">
    <div class="card-header align-items-center py-5 gap-2 gap-md-5">
        <div class="card-title">
            <div class="row">
                <div class="col-sm-12 col-md-5 d-flex align-items-center justify-content-center justify-content-md-start">
                    <div class="ms-10 fw-bold fs-4 text-gray-800">
                        @ViewBag.teacherName
                    </div>
                </div>
                <div class="col-sm-12 col-md-7 d-flex align-items-center justify-content-center justify-content-md-end">
                </div>
            </div>
        </div>
    </div>
    <div class="card-body pt-0">
        <div class="py-5">
            <div class="dataTables_wrapper dt-bootstrap4 no-footer">
                <div class="table-responsive">
                    <table class="table align-middle table-row-dashed fs-6 gy-5 dataTable no-footer">
                        <thead>
                            <tr class="text-start text-gray-400 fw-bold fs-7 text-uppercase gs-0">
                                <th class="text-start min-w-50px">ID</th>
                                <th class="text-start min-w-200px">STUDENT (F.L)</th>
                                <th class="text-start min-w-100px">LESSON TOPIC</th>
                                <th class="text-start min-w-100px">START DATE</th>
                                <th class="text-start min-w-100px">RANK</th>
                                <th class="text-start min-w-100px">COMMENT</th>
                            </tr>
                        </thead>
                        <tbody class="fw-semibold text-gray-800">
                            @{
                                foreach (var item in Model)
                                {

                                    <tr class="tr-shadow text-dark">
                                        <td class="text-start px-sm-0">
                                            <span class="fw-bold">@item.Id</span>
                                        </td>
                                        <td class="text-start px-sm-0 d-flex align-items-center">
                                            <div class="symbol symbol-circle symbol-50px overflow-hidden me-3">
                                                <div class="symbol-label">
                                                    @if (String.IsNullOrEmpty(item.ImagePath))
                                                    {
                                                        @if (item.LastName.EndsWith("a"))
                                                        {
                                                            <img src="~/media/avatars/150-7.jpg" alt="Emma Smith" class="w-100">
                                                        }
                                                        else
                                                        {
                                                            <img src="~/media/avatars/150-2.jpg" alt="Emma Smith" class="w-100" />
                                                        }
                                                    }
                                                    else
                                                    {
                                                        <img src="~/@item.ImagePath" alt="Emma Smith" class="w-100">
                                                    }
                                                    <img src="~" alt="Emma Smith" class="w-100">
                                                </div>
                                            </div>
                                            <span class="fw-bold text-hover-info text-gray-800 ms-3">@item.FirstName @item.LastName</span>
                                        </td>
                                        <td class="text-start">
                                            <span class="fw-bold text-gray-500">@item.LessonTopic</span>
                                        </td>
                                        <td class="text-start">
                                            <span class="fw-bold text-gray-500">@item.StartTime.ToString("dd-MM-yyyyㅤHH:mm")</span>

@using RegistanFerghanaLC.Service.ViewModels.TeacherViewModels;
@model List<TeacherByLessonsNumberViewModel>
@{
    ViewBag.Tab = 1;
}

<div id="kt_content_container" class="d-flex flex-column-fluid align-items-start">

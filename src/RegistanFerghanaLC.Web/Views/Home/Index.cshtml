                                                                        <img src="~/media/avatars/150-7.jpg" alt="" class="symbol w-45px h-45px me-3">
                                                                    }
                                                                    else
                                                                    {
                                                                        <img src="~/media/avatars/150-26.jpg" alt="" class="symbol w-45px h-45px me-3" />
                                                                    }
                                                                }
                                                                else
                                                                {
                                                                    <img src="~/@teacher.ImagePath" alt="" class="symbol w-45px h-45px me-3">
                                                                }
